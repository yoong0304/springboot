{{>layouts/header}}
    <table class="table">
        <thead>
            <tr>
                <th scope="col">ID</th>
                <th scope="col">Title</th>
                <th scope="col">Content</th>
            </tr>
        </thead>
        <tbody>
        {{#article}}
            <tr>
                <th>{{id}}</th>
                <th>{{title}}</th>
                <th>{{content}}</th>
            </tr>
        {{/article}}
        </tbody>
    </table>
    <a href="/articles/{{article.id}}/edit" class="btn btn-primary">수정</a>
    <a href="/articles/{{article.id}}/delete" class="btn btn-danger">삭제</a>
    <a href="/articles">Go to Article List</a>
<!--상세페이지에서 목록으로-->
{{>layouts/footer}}